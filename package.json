{
  "name": "buchiya4th-portfolio",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "buchiya4th <buchiya4th@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "precommit": "lint-staged",
    "lint": "run-p eslint stylelint",
    "lint-fix": "run-p eslint-fix stylelint-fix",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "eslint-fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "stylelint": "stylelint \"**/*.scss\"",
    "stylelint-fix": "stylelint \"**/*.scss\" --fix",
    "deploy": "git subtree push --prefix=dist/ origin master"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "^2.0.2",
    "axios": "^0.18.0",
    "nuxt": "^1.0.0",
    "vue": "^2.5.17",
    "vue-server-renderer": "^2.5.17",
    "vue-smoothscroll": "^0.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "nuxt-sass-resources-loader": "^2.0.4",
    "prettier": "^1.14.2",
    "pug": "2.0.3",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.5.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "url-loader": "^1.0.1"
  }
}
